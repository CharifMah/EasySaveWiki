{
  "metadata": [
    {
      "src": [
        {
          "src": "../G4-FISA-24/EasySave-2.0/EasySave-1.0/",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api/v1"
    },  
    {
      "src": [
        {
          "src": "../G4-FISA-24/EasySave-2.0/EasySaveGUI/",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api/v2"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "EasySaveWiki",
      "_appTitle": "EasySaveWiki",
      "_enableSearch": true,
      "pdf": true
    }
  }
}